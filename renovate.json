{
  "semanticCommits": true,
  "packageRules": [
    {
      "depTypeList": ["devDependencies"],
      "addLabels": ["dev-dependency"],
      "automerge": true,
    },
    {
      "matchPackagePrefixes": [
        "@types/"
      ],
      "automerge": true
    }
  ],
  "ignoreTests": true,
  "assignAutomerge": true,
  "automergeType": "pr",
  "ignoreTests": true,
  "extends": [
    "config:base"
  ]
}
